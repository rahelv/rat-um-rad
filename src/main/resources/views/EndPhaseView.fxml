<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" xmlns="http://javafx.com/javafx/19">
    <ImageView fitHeight="800.0" fitWidth="1000.0" layoutX="6.0" layoutY="-1.0" pickOnBounds="true">
        <Image url="@../images/basel_ohne_labels.png"/>
    </ImageView>
    <HBox layoutX="25.0" layoutY="46.0" prefHeight="307.0" prefWidth="494.0">
        <VBox prefHeight="299.0" prefWidth="203.0">
            <Label alignment="CENTER" text="Ranking list">
                <font>
                    <Font name="Arial Black" size="16.0"/>
                </font>
            </Label>
            <Label prefHeight="17.0" prefWidth="203.0" text="Rank / Player  /  score       ">
                <font>
                    <Font name="System Bold" size="13.0"/>
                </font>
            </Label>
            <ScrollPane prefHeight="285.0" prefWidth="203.0">
                <ListView fx:id="rankingListView" prefHeight="282.0" prefWidth="204.0"/>
            </ScrollPane>
        </VBox>
        <VBox prefHeight="307.0" prefWidth="293.0">
            <Label prefHeight="45.0" prefWidth="190.0" text="Your Achievements">
                <font>
                    <Font name="Arial Black" size="16.0"/>
                </font>
            </Label>
            <GridPane gridLinesVisible="true" prefHeight="225.0" prefWidth="292.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="140.0" minWidth="10.0" prefWidth="140.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="94.0" minWidth="10.0" prefWidth="75.0"/>
                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="96.0" minWidth="10.0" prefWidth="77.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints maxHeight="122.0" minHeight="10.0" prefHeight="34.0" vgrow="SOMETIMES"/>
                    <RowConstraints maxHeight="120.0" minHeight="10.0" prefHeight="89.0" vgrow="SOMETIMES"/>
                    <RowConstraints maxHeight="93.0" minHeight="10.0" prefHeight="81.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <Label text="  achieved goal cards" GridPane.rowIndex="1">
                    <font>
                        <Font name="Arial Bold" size="12.0"/>
                    </font>
                </Label>
                <Label fx:id="achievedGoalCardsNum" text="    Label" textFill="RED" GridPane.columnIndex="1"
                       GridPane.rowIndex="1"/>
                <Label fx:id="achievedGoalCardsScore" text="    Label" textFill="RED" GridPane.columnIndex="2"
                       GridPane.rowIndex="1"/>
                <Label fx:id="notAchievedGoalCardsNum" text="    Label" textFill="RED" GridPane.columnIndex="1"
                       GridPane.rowIndex="2"/>
                <Label fx:id="notAchievedGoalCardsScore" text="    Label" textFill="RED" GridPane.columnIndex="2"
                       GridPane.rowIndex="2"/>
                <Label text="  unachieved goal cards" GridPane.rowIndex="2">
                    <font>
                        <Font name="Arial Bold" size="12.0"/>
                    </font>
                </Label>
                <Label text="amount" GridPane.columnIndex="1">
                    <font>
                        <Font name="Arial Bold" size="12.0"/>
                    </font>
                </Label>
                <Label text="scores" GridPane.columnIndex="2">
                    <font>
                        <Font name="Arial Bold" size="12.0"/>
                    </font>
                </Label>
            </GridPane>
            <Label fx:id="totalScore" text="You got  totally ? points" textFill="RED">
                <font>
                    <Font name="Arial Black" size="16.0"/>
                </font>
            </Label>
        </VBox>
    </HBox>
    <Label layoutX="102.0" layoutY="389.0" prefHeight="26.0" prefWidth="109.0" text="Winner is  :">
        <font>
            <Font name="Arial Black" size="14.0"/>
        </font>
    </Label>
    <Label fx:id="winnerNameLabel" layoutX="222.0" layoutY="394.0" prefHeight="15.0" prefWidth="51.0" text="Label"
           textFill="BLUE">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
    <Label fx:id="winnerScoreLabel" layoutX="314.0" layoutY="394.0" text="Label" textFill="RED">
        <font>
            <Font size="14.0"/>
        </font>
    </Label>
</AnchorPane>
